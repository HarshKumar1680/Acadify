{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },

      {
        "name": "Launch HTML in Chrome",
        "type": "pwa-chrome",
        "request": "launch",
        "file": "${workspaceFolder}/index.html"
      }
    ]
  }
  